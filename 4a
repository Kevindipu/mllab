import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns
import plotly.graph_objs as go

# create some sample data
np.random.seed(1)
n = 100
data = np.random.randn(n, 4)

# create a box plot for each dimension
for i in range(4):
    plt.subplot(2, 2, i + 1)
    sns.boxplot(data[:, i])
    plt.title('Dimension {}'.format(i + 1))

# create a figure for plotly
fig = go.Figure()
for i in range(4):
    fig.add_trace(go.Box(x=data[:, i]))

# show the figures
plt.show()




import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
data=pd.read_csv('./ToyotaCorolla.csv')

#Box Plot for Price,Age,KM
plt.boxplot([data["Price"],data["Age"],data["KM"]])
plt.xticks([1,2,3],["Price","Age","KM"])
plt.show()
